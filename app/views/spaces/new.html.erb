<%= form_with (model: url: spaces_path, method: :post) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :decription%>
  <%= f.text_field :decription%>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :size %>
  <%= f.number_field :size %>
  <%= f.label :amenities %>
  <%= f.text_field :amenities %>
  <%= f.label :price %>
  <%= f.text_field :price %>
   <%= f.submit "Create Space" %>
<% end %>
